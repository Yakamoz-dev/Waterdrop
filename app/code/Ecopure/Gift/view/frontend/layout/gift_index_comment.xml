<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="Ecopure_Gift::css/giftcom.css" />
        <script src="Ecopure_Gift::js/giftcom.js"/>
    </head>
    <body>
        <referenceContainer name="content">
            <block class="Ecopure\Gift\Block\Giftcom" name="ecopure_giftcom" template="giftcom.phtml">
                <arguments>
                    <argument name="view_model" xsi:type="object">Ecopure\Gift\ViewModel\LoginHistory</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>