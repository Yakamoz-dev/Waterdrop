        <actionsColumn name="actions" class="Ecopure\Gift\Ui\Component\Listing\Columns\ProductEdit">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="resizeEnabled" xsi:type="boolean">false</item>
                    <item name="resizeDefaultWidth" xsi:type="string">120</item>
                    <item name="indexField" xsi:type="string">ro_id</item>
                </item>
            </argument>
        </actionsColumn>
    </columns>
</listing>
